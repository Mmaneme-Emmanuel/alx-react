{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-unused-vars": ["error"],
    "indent": ["error", 2],
    "quotes": ["error", "single"]
  }
}
